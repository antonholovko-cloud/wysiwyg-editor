{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-wysiwyg-editor": {
      "projectType": "library",
      "root": "projects/ngx-wysiwyg-editor",
      "sourceRoot": "projects/ngx-wysiwyg-editor/src",
      "prefix": "wysiwyg",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-wysiwyg-editor/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-wysiwyg-editor/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/ngx-wysiwyg-editor/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "bf22475c-2a99-4876-a67d-5e9b234ea912"
  }
}
